{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": "off",
      "no-console": "off",
      "class-methods-use-this": "off",
      "linebreak-style": "off"
    }
}
